{
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "nginx:latest",
            "portMappings": [
                { "containerPort": 80, "hostPort": 8080, "servicePort": 8080, "protocol": "tcp" }
            ]
        }
    },
    "id": "nginx",
    "instances": "1",
    "cpus": "0.5",
    "mem": "512",
    "ports": [8080],
    "uris": []
}