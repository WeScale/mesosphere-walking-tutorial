{
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "crosbymichael/dockerui",
            "network": "BRIDGE",
            "portMappings": [
                { "containerPort": 9000, "hostPort": 31500, "servicePort": 31500, "protocol": "tcp" }
            ]
        },
        "volumes": [
            {
                "hostPath": "/var/run/docker.sock",
                "containerPath": "/docker.sock",
                "mode": "RW"
            }
        ]
    },
    "args": ["-e /docker.sock"],
    "id": "dockerui",
    "instances": "1",
    "cpus": "0.5",
    "mem": "512",
    "uris": []
}